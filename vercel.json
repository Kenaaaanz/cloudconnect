{
  "version": 2,
  "builds": [
    {
      "src": "your_project_name/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "your_project_name/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "@",
    "DEBUG": "False"
  }
}